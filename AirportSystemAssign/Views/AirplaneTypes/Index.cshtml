@model IEnumerable<AirportSystemAssign.Models.AirplaneType>

@{
    ViewBag.Title = "Index";
}
<div class="whitetext">
    <h2>Index</h2>

    @*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    <a class="iconlinks" href="/AirplaneTypes/Delete/@item.Id"><span class="glyphicon glyphicon-trash"></span></a>
                    <a class="iconlinks" href="/AirplaneTypes/Details/@item.Id"><span class="glyphicon glyphicon-list-alt"></span></a>
                    <a class="iconlinks" href="/AirplaneTypes/Edit/@item.Id"><span class="glyphicon glyphicon-pencil"></span></a>
                </td>
            </tr>
        }

    </table>
</div>
<div>
    @Html.ActionLink("Back to Airports", "Index", "Airports")
</div>
